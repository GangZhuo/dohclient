
config cfg
	option bind_addr '127.0.0.1'
	option bind_port '53'
	option chnroute '/etc/dohclient/chnroute.txt,/etc/dohclient/chnroute6.txt'
	option timeout '30'
	option log_file '/var/log/dohclient.log'
	option log_level '5'
	option proxy '127.0.0.1:1080'
	option channel 'chndoh'
	option channel_args 'chndoh.addr=223.5.5.5:443&chndoh.host=dns.alidns.com&chndoh.path=/dns-query&frndoh.addr=8.8.8.8:443&frndoh.host=dns.google&frndoh.path=/dns-query&proxy=1&ecs=1&net=8.8.0.0/24&net6=2001:19f0:6401::/48'
